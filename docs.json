{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["introduction", "quickstart"]
          },
          {
            "group": "Essentials",
            "pages": ["essentials/markdown", "essentials/code", "essentials/images", "essentials/settings", "essentials/navigation"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "https://stoplight.io/api/v1/projects/contxt/live-risk/nodes/live_risk_docs_v2.yaml",
        "groups": [
          {
            "group": "API Documentation",
            "pages": ["api-reference/authentication"]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/update",
              "api-reference/endpoint/delete"
            ]
          }
        ]
      },
      {
        "tab": "PTA API",
        "openapi": "https://stoplight.io/api/v1/projects/contxt/paxafe-api/nodes/PAXAFE-PTA-Device-API-2v1v7.yaml",
        "groups": [
          {
            "group": "Arrival Prediction",
            "pages": ["pta/introduction"]
          }
        ]
      },
      {
        "tab": "Lane Risk API",
        "openapi": "https://stoplight.io/api/v1/projects/contxt/paxafe-api/nodes/CONTXT-Lane-Risk-Assessment-7v0v1.yaml",
        "groups": [
          {
            "group": "Lane Risk Assessment",
            "pages": ["lane-risk/introduction"]
          }
        ]
      },
      {
        "tab": "PTE API",
        "openapi": "https://stoplight.io/api/v1/projects/contxt/paxafe-api/nodes/PAXAFE-PTE-1v1v0.yaml",
        "groups": [
          {
            "group": "Temperature Prediction",
            "pages": ["pte/introduction"]
          }
        ]
      },
      {
        "tab": "Authorization API",
        "openapi": "https://stoplight.io/api/v1/projects/contxt/paxafe-api/nodes/CONTXT-Authorization-5v0v0.yaml",
        "groups": [
          {
            "group": "Authentication",
            "pages": ["auth/introduction"]
          }
        ]
      }
    ]
  },
  "topbarCtaButton": {
    "name": "Get Started",
    "url": "https://dashboard.mintlify.com"
  },
  "topbarLinks": [
    {
      "name": "Log In",
      "url": "https://dashboard.mintlify.com/login"
    }
  ],
  "anchors": [
    {
      "name": "API Reference",
      "icon": "rectangle-terminal",
      "url": "api-reference"
    },
    {
      "name": "Changelog",
      "icon": "list",
      "url": "https://mintlify.com"
    },
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://mintlify.com/community"
    }
  ],
  "versions": ["v2.0", "v1.0"],
  "backgroundImage": "/background.png",
  "integrations": {
    "intercom": {
      "appId": "abc123456"
    }
  },
  "openapi": "https://example.com/openapi.json",
  "api": {
    "baseUrl": "https://api.example.com",
    "auth": {
      "method": "key",
      "name": "Authorization",
      "inputPrefix": "Bearer "
    }
  },
  "search": {
    "prompt": "Search documentation...",
    "placeholder": "Search...",
    "emptyResult": "No results found."
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}